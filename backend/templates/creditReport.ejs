        padding: 12px 15px;
        border-bottom: 1px solid #eee;
        text-align: left;
      }

      .contact-table tbody tr:last-child td {
        border-bottom: none;
      }

      .contact-table tbody tr:nth-child(even) {
        background: #f9fafe;
      }

      .phone-email-container {
        display: flex;
        gap: 20px;
        flex-wrap: wrap; /* Allows wrapping on smaller screens */
      }

      .phone-box,
      .email-box {
        flex: 1; /* Allows boxes to grow and shrink */
        min-width: 250px; /* Minimum width before wrapping */
        border: 1px solid #e3e3e3;
        border-radius: 12px;
        background: #ffffff;
        padding: 0; /* Tables will handle padding */
        overflow: hidden;
      }

      .phone-box h3,
      .email-box h3 {
        font-size: 16px;
        color: #1c2d41;
        padding: 12px 15px;
        margin: 0;
        background: #eef3fc;
        border-bottom: 1px solid #eee;
      }
